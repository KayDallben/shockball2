{
  "name": "shockball2",
  "version": "0.0.1",
  "engines": {
    "node": "8.9.4"
  },
  "description": "second iteration of shockball for star wars combine",
  "scripts": {
    "build": "npm run buildClient && npm run copyClientBuild && npm run buildServer",
    "start": "cross-env NODE_ENV=production nodemon server/dist",
    "dev": "npm run build && cross-env NODE_ENV=development nodemon server/dist",
    "coverage": "jest --coverage",
    "test": "jest",
    "lintMac": "DEBUG=eslint:cli-engine && eslint server/src --ignore-pattern client/src/docs/",
    "lintWindows": "set DEBUG=eslint:cli-engine && eslint server/src --ignore-pattern client/src/docs/",
    "precommit": "npm run lintWindows",
    "buildClient": "cd client && npm run build",
    "copyClientBuild": "ncp client/build public",
    "startClient": "cd client && npm run start",
    "buildServer": "babel server/src -s -D -d server/dist --presets es2015,stage-0 --ignore server/src/**/*.test.js",
    "startServer": "nodemon -w server/src --exec \"babel-node server/src --presets es2015,stage-0\""
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-jest": "^21.2.0",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "chance": "^1.0.13",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "director": "^1.2.8",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.2",
    "express": "^4.16.2",
    "fantasy-names": "^2.0.0",
    "firebase": "^4.9.0",
    "firebase-admin": "^5.8.1",
    "firestore-backup-restore": "^1.3.1",
    "helmet": "^3.9.0",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "joi": "^13.0.2",
    "mobx-utils": "^3.2.2",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "ncp": "^2.0.0",
    "nodemon": "^1.12.6",
    "querystring": "^0.2.0",
    "serialize-error": "^2.1.0",
    "serve-favicon": "^2.4.5",
    "supertest": "^3.0.0",
    "swagger-jsdoc": "^1.9.7",
    "uuid": "^3.2.1",
    "winston": "^2.4.0"
  }
}
