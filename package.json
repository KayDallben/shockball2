{
  "name": "shockball2",
  "version": "0.0.1",
  "description": "second iteration of shockball for star wars combine",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0 --ignore src/**/*.test.js",
    "start": "cross-env NODE_ENV=production npm run build && nodemon dist",
    "lint": "eslint src",
    "test": "jest",
    "coverage": "jest --coverage",
    "precommit": "npm run lint && npm test"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "helmet": "^3.9.0",
    "joi": "^13.0.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.6",
    "serve-favicon": "^2.4.5",
    "swagger-jsdoc": "^1.9.7",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.2",
    "husky": "^0.14.3",
    "babel-preset-jest": "^21.2.0",
    "babel-jest": "^21.2.0",
    "jest": "^21.2.1",
    "supertest": "^3.0.0"
  }
}
